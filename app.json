{
  "expo": {
    "name": "Stride Quest",
    "slug": "stride-quest",
    "version": "0.6.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0d12"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stridequest.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to track your activities like running, walking, and cycling.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to track your activities like running, walking, and cycling."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0f0d12"
      },
      "package": "com.stridequest.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Stride Quest to use your location to track your activities."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "97f40020-ea2c-43d5-aa89-41c62945d77c"
      }
    }
  }
}
